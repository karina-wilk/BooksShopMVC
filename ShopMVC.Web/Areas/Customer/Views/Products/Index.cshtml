@model IEnumerable<ShopMVC.Web.Areas.AdminArea.Models.BookDisplayModel>

@{
    ViewBag.Title = "Index";
}

<h2>Products</h2>

@if(TempData["Message"] != null)
{
    <div class="alert alert-info">@TempData["Message"]</div>
}
@foreach (var book in Model)
{
    @Html.Partial("BookTileView", book);
}
<div class="clearfix"></div>